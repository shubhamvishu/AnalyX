<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="512.0" prefWidth="420.0" style="-fx-background-color: #212F3C;" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InsertShop">
   <children>
      <JFXButton fx:id="addshop" layoutX="32.0" layoutY="439.0" onAction="#add" prefHeight="52.0" prefWidth="169.0" style="-fx-background-color: #06f141; -fx-background-radius: 30px; -fx-border-radius: 30px;" text="  Add">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT">
               <image>
                  <Image url="@../img/icons8_Plus_Math_64px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXTextField fx:id="tsid" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="148.0" prefHeight="37.0" prefWidth="355.0" promptText="SID" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#c2bbbb" />
      <JFXTextField fx:id="tsname" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="217.0" prefHeight="37.0" prefWidth="355.0" promptText="Shop Name" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
      <JFXTextField fx:id="trev" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="283.0" prefHeight="37.0" prefWidth="355.0" promptText="Revenue" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
      <JFXTextField fx:id="tcatid" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="348.0" prefHeight="37.0" prefWidth="355.0" promptText="CATID" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
      <JFXButton fx:id="cancel" layoutX="220.0" layoutY="439.0" onAction="#cancelInsert" prefHeight="52.0" prefWidth="169.0" style="-fx-background-color: #dadada#34495E; -fx-background-radius: 30px; -fx-border-radius: 30px;" text=" Cancel">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0">
               <image>
                  <Image url="@../img/icons8_Delete_64px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <ImageView layoutX="140.0" layoutY="31.0">
         <image>
            <Image url="@../img/icons8_Shop_96px.png" />
         </image>
      </ImageView>
      <ImageView layoutX="204.0" layoutY="31.0">
         <image>
            <Image url="@../img/icons8_Small_Business_96px.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>

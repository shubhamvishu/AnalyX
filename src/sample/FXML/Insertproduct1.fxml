<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="555.0" prefWidth="444.0" style="-fx-background-color: #333;" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InsertProduct">
   <children>
      <JFXButton fx:id="addprod" layoutX="33.0" layoutY="476.0" onAction="#add" prefHeight="52.0" prefWidth="169.0" style="-fx-background-color: #06f141; -fx-background-radius: 30px; -fx-border-radius: 30px;" text="  Add">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT">
               <image>
                  <Image url="@../img/icons8_Plus_Math_64px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXTextField fx:id="tpid" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="148.0" prefHeight="37.0" prefWidth="355.0" promptText="PID" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#c2bbbb" />
      <JFXTextField fx:id="tpname" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="217.0" prefHeight="37.0" prefWidth="355.0" promptText="Product Name" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
      <JFXTextField fx:id="tpcost" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="283.0" prefHeight="37.0" prefWidth="355.0" promptText="Cost" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
      <JFXTextField fx:id="tprofit" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="348.0" prefHeight="37.0" prefWidth="355.0" promptText="Profit" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
      <JFXButton fx:id="cancel" layoutX="221.0" layoutY="476.0" onAction="#cancelInsert" prefHeight="52.0" prefWidth="169.0" style="-fx-background-color: #dadada#34495E; -fx-background-radius: 30px; -fx-border-radius: 30px;" text=" Cancel">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0">
               <image>
                  <Image url="@../img/icons8_Delete_64px.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <ImageView layoutX="162.0" layoutY="20.0">
         <image>
            <Image url="@../img/icons8_Add_User_Male_96px.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="tcatid" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="408.0" prefHeight="37.0" prefWidth="355.0" promptText="Catid" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
   </children>
</AnchorPane>

            </ImageView>
         </graphic>
      </JFXButton>
      <JFXTextField fx:id="toid" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="148.0" prefHeight="37.0" prefWidth="355.0" promptText="OID" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#c2bbbb" />
      <JFXTextField fx:id="toname" focusColor="#02eee6" labelFloat="true" layoutX="34.0" layoutY="234.0" prefHeight="37.0" prefWidth="355.0" promptText="Owner Name" styleClass="jfx-customer-textfield" stylesheets="@../style.css" unFocusColor="#d0c9c9" />
